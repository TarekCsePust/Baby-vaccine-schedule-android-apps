<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hasantarek.vaccinedetailasandschedule">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddHuman" />
        <activity android:name=".NameList" />
        <activity android:name=".AddChild" />
        <activity android:name=".VaccineInfoList" />

        <receiver android:name=".Notification" />

        <activity android:name=".VaccineDetails"></activity>
    </application>

</manifest>
